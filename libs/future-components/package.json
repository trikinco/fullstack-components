{
	"name": "@trikinco/fullstack-components",
	"version": "1.0.13",
	"description": "A collection of Fullstack and AI powered components",
	"exports": {
		".": "./dist/index.js",
		"./client": "./dist/client/index.js",
		"./utils": "./dist/utils/index.js"
	},
	"author": "trikinco",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/trikinco/fullstack-components/issues"
	},
	"homepage": "https://github.com/trikinco/fullstack-components#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/trikinco/fullstack-components.git"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"browser": {
		"client": "dist/client/index.js",
		"utils": "dist/utils/index.js"
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/github",
				{
					"assets": []
				}
			],
			"@semantic-release/npm"
		]
	},
	"files": [
		"dist",
		"src",
		"package.json",
		"README.md",
		"client.js",
		"client.d.ts"
	],
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"encoding": "0.1.13",
		"next": "14.0.1",
		"openai": "4.14.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"sitemapper": "3.2.6"
	},
	"devDependencies": {
		"@types/node": "20",
		"@types/react": "18.2.31",
		"@types/react-dom": "18.2.14",
		"@typescript-eslint/eslint-plugin": "6.7.0",
		"@typescript-eslint/parser": "6.7.0",
		"eslint": "8.52.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-import": "2.28.1",
		"eslint-plugin-jest": "27.4.0",
		"eslint-plugin-jsdoc": "46.8.1",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-sonarjs": "0.21.0",
		"eslint-plugin-unicorn": "48.0.1",
		"jest": "29.7.0",
		"typescript": "5.2.2"
	},
	"peerDependencies": {
		"next": ">=14",
		"react": ">=18",
		"react-dom": ">=18"
	}
}
